# 1inch API Configuration
# Used by Vite dev proxy to authenticate with 1inch API (server-side)
ONEINCH_API_KEY=your_1inch_api_key_here

# Optional: Custom API endpoints (defaults shown)
# ONEINCH_BASE_URL=https://api.1inch.dev
# VITE_PROXY_BASE=/api/1inch

# Optional: API rate limiting configuration (defaults shown)
# VITE_API_REQUEST_INTERVAL=100
# VITE_MAX_API_RETRIES=5

# Reown Project ID (formerly WalletConnect - get from https://dashboard.reown.com/)
VITE_REOWN_PROJECT_ID=your_project_id_here
# Legacy support for old WalletConnect project ID
# VITE_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id_here

# Optional: Default settings
VITE_DEFAULT_TARGET_TOKEN=USDC
VITE_DEFAULT_CHAIN=base
VITE_DEFAULT_MIN_USD_VALUE=5
