{
  "version": 2,
  "buildCommand": "bun run build:web",
  "outputDirectory": "dist-web",
  "installCommand": "bun install",
  "framework": null,
  "rewrites": [
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
