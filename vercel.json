{
  "version": 2,
  "buildCommand": "tsc && bun run build:web",
  "outputDirectory": "dist-web",
  "installCommand": "bun install",
  "rewrites": [
    {
      "source": "/api/1inch/(.*)",
      "destination": "/api/1inch/[...path].ts"
    },
    {
      "source": "/api/(.*)",
      "destination": "api/$1.ts"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/index.html"
    }
  ]
}